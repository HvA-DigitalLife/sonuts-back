{
	"AllowedHosts": "*",
	"Authentication": {
		"Audience": "<Audience>",
		"Issuer": "<Issuer>",
		"SecurityKey": "<SecurityKey>"
	},
	"ConnectionStrings": {
		"PostgreSQL": "<DefaultConnection>",
		"Fhir": "<Fhir>"
	},
	"Database": {
		"SensitiveDataLogging": false
	},
	"Fhir": {
		"Read": false,
		"Write": false
	},
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"Swagger": false,
	"UseInMemoryDatabase": false
}
